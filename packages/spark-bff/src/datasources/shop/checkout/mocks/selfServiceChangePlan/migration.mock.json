{
  "sections": [
    {
      "sectionId": "account-setup",
      "name": "Account",
      "steps": [
        {
          "stepId": "personalDetails",
          "name": "Personal details",
          "fields": [
            {
              "name": "personal_details_description",
              "type": "content_field",
              "fieldRule": { "required": { "expression": "false" } },
              "label": "Personal details description",
              "customAttributes": [
                {
                  "key": "contentKey",
                  "value": "checkout.account.personalDetails.description"
                }
              ]
            },
            {
              "name": "first_name",
              "type": "textfield",
              "fieldRule": {
                "validations": {
                  "regex": {
                    "expression": "^[A-Za-z\\s.-]{1,50}$",
                    "message": "Please enter no more than 50 letters"
                  }
                }
              },
              "label": "First name",
              "datatype": "String",
              "customAttributes": [
                { "key": "shouldMask", "value": "true" },
                { "key": "showInSummary", "value": "true" },
                { "key": "summaryLabel", "value": "Name" },
                {
                  "key": "summaryTemplate",
                  "value": "first_name + ' ' + last_name"
                },
                { "key": "placeholder", "value": "Your first name" }
              ]
            },
            {
              "name": "middle_name",
              "type": "textfield",
              "fieldRule": {
                "required": { "expression": "false" },
                "validations": {
                  "regex": {
                    "expression": "^[A-Za-z\\s.-]{1,50}$",
                    "message": "Please enter no more than 50 letters"
                  }
                }
              },
              "label": "Middle name",
              "datatype": "String",
              "customAttributes": [
                { "key": "placeholder", "value": "Your middle name" },
                { "key": "shouldMask", "value": "true" }
              ]
            },
            {
              "name": "last_name",
              "type": "textfield",
              "fieldRule": {
                "validations": {
                  "regex": {
                    "expression": "^[A-Za-z\\s.-]{1,50}+$",
                    "message": "Please enter no more than 50 letters"
                  }
                }
              },
              "label": "Last name",
              "datatype": "String",
              "customAttributes": [
                { "key": "placeholder", "value": "Your last name" },
                { "key": "shouldMask", "value": "true" }
              ]
            },
            {
              "name": "date_of_birth",
              "type": "full_date_input",
              "label": "Date of birth",
              "datatype": "String",
              "customAttributes": [
                { "key": "shouldMask", "value": "true" },
                { "key": "showInSummary", "value": "true" },
                { "key": "summaryLabel", "value": "Date of birth" },
                {
                  "key": "summaryTemplate",
                  "value": "formatDate(date_of_birth, 'dd/MM/yyyy')"
                }
              ]
            },
            {
              "name": "email",
              "type": "textfield",
              "fieldRule": {
                "validations": {
                  "regex": {
                    "expression": "^([a-zA-Z0-9_\\-\\.]+)@([a-zA-Z0-9_\\-\\.]+)\\.([a-zA-Z]{2,5})$",
                    "message": "Please check if your email address is correct. Example: johnsmith@gmail.com"
                  }
                }
              },
              "label": "Email",
              "datatype": "String",
              "customAttributes": [
                { "key": "placeholder", "value": "Your email" },
                { "key": "shouldMask", "value": "true" },
                { "key": "showInSummary", "value": "true" },
                { "key": "summaryLabel", "value": "Email" },
                { "key": "summaryTemplate", "value": "email" }
              ]
            },
            {
              "name": "contact_number",
              "type": "phone_input",
              "fieldRule": {
                "validations": {
                  "regex": {
                    "expression": "^[0-9]{9,12}$",
                    "message": "Enter from 6 to 9 digits"
                  }
                }
              },
              "label": "Contact number",
              "datatype": "String",
              "customAttributes": [
                { "key": "shouldMask", "value": "true" },
                { "key": "showInSummary", "value": "true" },
                { "key": "summaryLabel", "value": "Contact number" },
                { "key": "summaryTemplate", "value": "contact_number" },
                { "key": "placeholder", "value": "Your number" },
                {
                  "key": "customItems",
                  "value": "021, 023, 022, 027, 028, 029, 0204"
                },
                { "key": "defaultValue", "value": "023" }
              ]
            },
            {
              "name": "chooseYourID",
              "type": "radioOption",
              "label": "Choose your ID",
              "datatype": "String",
              "options": [
                {
                  "value": "NZ driver licence",
                  "fields": [
                    {
                      "name": "your_driver_licence",
                      "type": "heading_field",
                      "fieldRule": { "required": { "expression": "false" } },
                      "label": "Your NZ driver Licence",
                      "customAttributes": [
                        {
                          "key": "infoLayerContentKey",
                          "value": "checkout.account.driverLicence.infoLayer"
                        },
                        {
                          "key": "regionId",
                          "value": "account.nzDriverLicence"
                        }
                      ]
                    },
                    {
                      "name": "licence_number",
                      "type": "textfield",
                      "fieldRule": {
                        "validations": {
                          "regex": {
                            "expression": "^\\w{8}$",
                            "message": "Your licence number should be 8 characters with no punctuation or symbols"
                          }
                        }
                      },
                      "label": "Licence number",
                      "datatype": "String",
                      "customAttributes": [
                        { "key": "shouldMask", "value": "true" },
                        { "key": "showInSummary", "value": "true" },
                        { "key": "summaryLabel", "value": "Licence number" },
                        { "key": "summaryTemplate", "value": "licence_number" },
                        { "key": "placeholder", "value": "Your licence number" }
                      ]
                    },
                    {
                      "name": "licence_version_number",
                      "type": "textfield",
                      "fieldRule": {
                        "validations": {
                          "regex": {
                            "expression": "^[0-9]{3}$",
                            "message": "Your licence version number should be 3 numbers only, with no punctuation or symbols"
                          }
                        }
                      },
                      "label": "Licence version number",
                      "datatype": "String",
                      "customAttributes": [
                        { "key": "shouldMask", "value": "true" },
                        { "key": "showInSummary", "value": "true" },
                        { "key": "summaryLabel", "value": "Version number" },
                        {
                          "key": "summaryTemplate",
                          "value": "licence_version_number"
                        },
                        {
                          "key": "placeholder",
                          "value": "Your licence version number"
                        }
                      ]
                    },
                    {
                      "name": "licence_exp_date",
                      "type": "full_date_input",
                      "label": "Licence expiry date",
                      "datatype": "String",
                      "customAttributes": [
                        { "key": "shouldMask", "value": "true" },
                        { "key": "showInSummary", "value": "true" },
                        { "key": "summaryLabel", "value": "Expiry date" },
                        {
                          "key": "summaryTemplate",
                          "value": "formatDate(licence_exp_date, 'dd/MM/yyyy')"
                        }
                      ]
                    },
                    {
                      "name": "applicationId",
                      "type": "textfield",
                      "fieldRule": {
                        "visibility": { "expression": "false" },
                        "required": { "expression": "false" }
                      },
                      "label": "Application ID",
                      "datatype": "String"
                    },
                    {
                      "name": "submitNZDriverLicence",
                      "type": "button",
                      "fieldRule": { "required": { "expression": "false" } },
                      "label": "Save and continue",
                      "datatype": "Boolean"
                    }
                  ]
                },
                {
                  "value": "NZ passport",
