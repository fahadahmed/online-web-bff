{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "skipLibCheck": true,
    "noEmit": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
